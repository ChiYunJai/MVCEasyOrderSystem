@using MvcEasyOrderSystem.ViewModels
@model List<Group<string, int>>

@*@model IEnumerable<System.Linq.IGrouping<string, MvcEasyOrderSystem.Models.Category>>*@
<ol>
    @foreach (var item in Model)
    {
        <li>@Html.ActionLink(string.Format("{0}({1})",@item.Key.ToString(), @item.Value),
        "ShowByCategory", new {categoryId = item.Id})
        </li>     
    }
</ol>
